<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="tugas_belajar.tugas_belajar_rule_komisi_karyawan_wizard">
            <field name="name">Wizard Rule Komisi Karyawan</field>
            <field name="model">tugas_belajar.tugas_belajar_rule_komisi_karyawan_wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="product"/>
                        </group>
                        <group>
                            <field name="persentase"/>
                        </group>
                    </sheet>
                    <footer>
                        <button string="Tambah" name="save" type="object" class="btn-primary"/>
                        <button string="Batal" special="cancel" class="btn-secondary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
