<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="tugas_belajar.history_komisi_karyawan_list">
            <field name="name">tugas_belajar history komisi karyawan list</field>
            <field name="model">tugas_belajar.tugas_belajar_history_komisi_karyawan</field>
            <field name="arch" type="xml">
                <tree create="0">
                    <field name="date"/>
                    <field name="karyawan"/>
                    <field name="so"/>
                    <field name="jumlah_komisi" widget="monetary"/>
                    <field name="currency_id" invisible="1"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
