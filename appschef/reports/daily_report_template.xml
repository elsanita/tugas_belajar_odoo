<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <template id="daily_report">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.basic_layout">
                    <div class="page">
                        <div class="text-center">
                            <h1>Daily Report</h1>
                        </div>
                        <tr>
                            <td>
                                <span t-field="o.karyawan"/>
                            </td>
                            <td>
                                <span t-field="o.tanggal"/>
                            </td>
                        </tr>
                        <!-- <h3>Date:
                            <span t-field="o.tanggal"/>
                        </h3>
                        <h3>Karyawan:
                            <span t-field="o.karyawan"/>
                        </h3> -->
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>